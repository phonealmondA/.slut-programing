* <main> LearnToSolve {
    ^ observe_execution {
	
        targetNum <> 250
        firstInput <> 3
        secondInput <> 7
        
        speak("Searching for: ~targetNum~ using [~firstInput~, ~secondInput~, ?]")
        
        result([targetNum]) <> randomChoice([secondInput, firstInput, ?])
        
        speak("RESULT: ~result~")
        
        woof result
    }
}